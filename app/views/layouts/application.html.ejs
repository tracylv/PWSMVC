    <body>

    <!-- header start  -->
    <%= partial('layout_header'); %>
    <!-- header end  -->

    <!-- .container start  -->
    <div class="container">
      <%- displayFlash(flash); %>
      <%- render(); %>
    </div>
    <!-- .container end  -->

    <!-- footer start  -->
    <%= partial('layout_footer'); %>
    <!-- footer end  -->

